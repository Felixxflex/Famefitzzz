<div class="formceleb">
      <%= simple_form_for @celeb do |f| %>

      <div class="form-inputs">
        <%= f.label :name, class: 'text-white' %>
        <%= f.input :name %>
        <%= f.label :date, class: 'text-white' %>
        <%= f.date_field :date? %>
        <%= f.label :photos, class: 'text-white' %>
        <%= f.file_field :photos, multiple: true %>
        <%= f.label :shop_title, class: 'text-white' %>
        <%= f.text_field :shop_title %>
        <%= f.label :description, class: 'text-white' %>
        <%= f.text_field :description %>
        <%= f.label :title_link2, class: 'text-white' %>
        <%= f.text_field :title_link2 %>
        <%= f.label :description_link2, class: 'text-white' %>
        <%= f.text_field :description_link2 %>
        <%= f.label :title_link3, class: 'text-white' %>
        <%= f.text_field :title_link3 %>
        <%= f.label :description_link3, class: 'text-white' %>
        <%= f.text_field :description_link3 %>
        <%= f.label :title_link4, class: 'text-white' %>
        <%= f.text_field :title_link4 %>
        <%= f.label :description_link4, class: 'text-white' %>
        <%= f.text_field :description_link4 %>
        <%= f.label :title_link5, class: 'text-white' %>
        <%= f.text_field :title_link5 %>
        <%= f.label :description_link5, class: 'text-white' %>
        <%= f.text_field :description_link5 %>
        <%= f.label :title_link6, class: 'text-white' %>
        <%= f.text_field :title_link6 %>
        <%= f.label :description_link6, class: 'text-white' %>
        <%= f.text_field :description_link6 %>
        <%= f.label :title_link7, class: 'text-white' %>
        <%= f.text_field :title_link7 %>
        <%= f.label :description_link7, class: 'text-white' %>
        <%= f.text_field :description_link7 %>
        <%= f.label :title_link8, class: 'text-white' %>
        <%= f.text_field :title_link8 %>
        <%= f.label :description_link8, class: 'text-white' %>
        <%= f.text_field :description_link8 %>
      </div>


        <%= f.submit "Submit offer", class: "btn btn-primary btn-block" %>

      <% end %>

</div>